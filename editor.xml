<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="15" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="IS_MOUSE_CLICK_SELECTION_HONORS_CAMEL_WORDS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="question" useCustomColors="true" pattern="\bTODO\b.*kee\b.*">
      <option name="FOREGROUND" value="d2d2d2" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="a94e7" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bFIXME\b.*kee\b.*">
      <option name="FOREGROUND" value="d2d2d2" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="a94e7" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bDEV\b.*kee\b.*">
      <option name="FOREGROUND" value="d2d2d2" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="a94e7" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
    <filter name="my">
      <pattern index="4" />
      <pattern index="2" />
      <pattern index="3" />
    </filter>
  </component>
</application>